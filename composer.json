            "name": "Kento Oka",
            "email": "oka.kento0311@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "psr/container": "^1.0",
        "psr/log": "^1.0",
        "fratily/router": "^0.3.0",
        "fratily/http-message": "^0.1.0",
        "fratily/http-message-factory": "^0.2.0",
        "fratily/http-server-middleware": "^0.2.0",
        "fratily/reflection": "^0.1.0",
        "fratily/utility": "^0.1.0",
        "twig/twig": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
